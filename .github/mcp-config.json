{
  "repository": {
    "name": "WatchLaterZap",
    "description": "Chrome extension to delete all YouTube Watch Later videos with one click",
    "private": false,
    "default_branch": "main"
  },
  "commit_settings": {
    "auto_push": true,
    "commit_message_template": "feat: {message}"
